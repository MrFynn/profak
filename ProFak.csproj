<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <Authors>Łukasz Kosson</Authors>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="profak-demo.probak" />
    <None Remove="profak-start.probak" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="profak-demo.probak">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="profak-start.probak">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="5.0.10">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="5.0.10" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="UI\NumeratorSpis.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="UI\StanNumeratoraSpis.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="UI\StanNumeratoraEdytor.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="UI\WplataSpis.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="UI\WplataEdytor.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="UI\PozycjaFakturySpis.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="UI\FakturaZakupuSpis.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="UI\FakturaSprzedazySpis.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="UI\FakturaEdytor.cs">
      <SubType>UserControl</SubType>
    </Compile>
    <Compile Update="UI\TowarSpis.cs">
      <SubType>Component</SubType>
    </Compile>
  </ItemGroup>

</Project>